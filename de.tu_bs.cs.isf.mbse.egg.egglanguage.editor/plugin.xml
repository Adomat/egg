            icon="icons/full/obj16/GameelementsModelFile.gif">
         <description>%_UI_GameelementsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated egglanguage -->
      <editor
            id="egglanguage.gameelements.presentation.GameelementsEditorID"
            name="%_UI_GameelementsEditor_label"
            icon="icons/full/obj16/GameelementsModelFile.gif"
            extensions="gameelements"
            class="egglanguage.gameelements.presentation.GameelementsEditor"
            contributorClass="egglanguage.gameelements.presentation.GameelementsActionBarContributor">
      </editor>
   </extension>

</plugin>
